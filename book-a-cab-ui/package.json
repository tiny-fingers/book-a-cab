{
  "name": "backup-book-a-cab-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "^2.0.0",
    "@okta/okta-auth-js": "^7.5.1",
    "@okta/okta-react": "^6.7.0",
    "axios": "^0.27.2",
    "eslint": "^8.57.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "serve": "^14.2.1"
  },
  "scripts": {
    "start": "cross-env PORT=${SERVER_PORT} REACT_APP_API_SERVER_URL=${REACT_APP_API_SERVER_URL} react-scripts start",
    "build": "react-scripts build",
    "clean": "rm -rf build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "api": "json-server --host 0.0.0.0 --port 6060 json-server/db.json --routes json-server/routes.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/prop-types": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "json-server": "^0.17.0",
